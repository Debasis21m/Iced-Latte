{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "userId": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "items": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "productInfo": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string"
              },
              "name": {
                "type": "string"
              },
              "imageLink": {
                "type": "string"
              },
              "description": {
                "type": "string"
              },
              "price": {
                "type": "number"
              },
              "quantity": {
                "type": "number"
              },
              "active": {
                "type": "boolean"
              }
            }
          },
          "productQuantity": {
            "type": "integer"
          }
        }
      }
    },
    "itemsQuantity": {
      "type": "number"
    },
    "itemsTotalPrice": {
      "type": "number"
    },
    "deliveryCost": {
      "type": "number"
    },
    "taxCost": {
      "type": "number"
    },
    "totalOrderCost": {
      "type": "number"
    },
    "deliveryInfo": {
      "type": "string"
    },
    "recipientName": {
      "type": "string"
    },
    "recipientSurName": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "phone": {
      "type": "number"
    },
    "createdAt": {
      "type": "string"
    }
  }
}